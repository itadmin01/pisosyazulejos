<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="res_config_settings_view_form_inh" model="ir.ui.view">
            <field name="name">sale res config setting</field>
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="sale.res_config_settings_view_form"/>
            <field name="arch" type="xml">
            	<xpath expr="//field[@name='auth_signup_uninvited']" position="after">
            			<div class="o_setting_left_pane">
                        </div>
            			<div class="o_setting_right_pane">
            			</div>
                       	<label for="lista_precio"/>
                           <div class="mt16">
                           		<field name="lista_precio" class="o_light_label"/>
                           </div>
              		</xpath>			 
	            </field>
	        </record>
        </data>	
        
</odoo>